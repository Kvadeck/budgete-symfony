{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}

    {% if is_token_found %}
        <h1 class="h2">{{ title }}</h1>
        <p>Type a new password.</p>
        {{ form(form) }}
    {% else %}
        <h1 class="h2">Error</h1>
        <p>Something goes wrong.</p>
    {% endif %}

{% endblock %}